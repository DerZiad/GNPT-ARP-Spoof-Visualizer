<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="699.0" prefWidth="1314.0" style="-fx-background-color: white;"
      xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.npt.controllers.MainController">
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="Sniffing">
            <MenuItem mnemonicParsing="false" text="Start Sniffing"/>
            <MenuItem mnemonicParsing="false" text="Stop Sniffing"/>
        </Menu>
        <Menu mnemonicParsing="false" text="FilePapa">
            <MenuItem mnemonicParsing="false" text="New"/>
            <MenuItem mnemonicParsing="false" text="Open…"/>
            <Menu mnemonicParsing="false" text="Open Recent"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Close"/>
            <MenuItem mnemonicParsing="false" text="Save"/>
            <MenuItem mnemonicParsing="false" text="Save As…"/>
            <MenuItem mnemonicParsing="false" text="Revert"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Preferences…"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Quit"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Undo"/>
            <MenuItem mnemonicParsing="false" text="Redo"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Cut"/>
            <MenuItem mnemonicParsing="false" text="Copy"/>
            <MenuItem mnemonicParsing="false" text="Paste"/>
            <MenuItem mnemonicParsing="false" text="Delete"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Select All"/>
            <MenuItem mnemonicParsing="false" text="Unselect All"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About MyHelloApp"/>
        </Menu>
    </MenuBar>
    <AnchorPane fx:id="settingButton" prefHeight="195.0" prefWidth="1314.0">
        <Label layoutX="14.0" layoutY="14.0" text="Interface"/>
        <MenuButton fx:id="menuButton" layoutX="186.0" layoutY="10.0" mnemonicParsing="false"/>
        <Label layoutX="14.0" layoutY="53.0" prefHeight="18.0" prefWidth="127.0" text="Device Name"/>
        <TextField fx:id="deviceName" layoutX="186.0" layoutY="49.0" prefHeight="26.0" prefWidth="168.0"/>
        <Label layoutX="14.0" layoutY="91.0" text="IP Address"/>
        <TextField fx:id="ipAddress" layoutX="186.0" layoutY="87.0" prefHeight="26.0" prefWidth="168.0"/>
        <Button fx:id="addDevice" layoutX="432.0" layoutY="49.0" mnemonicParsing="false" prefHeight="26.0"
                prefWidth="162.0" text="Add Device"/>
    </AnchorPane>
    <AnchorPane fx:id="rootAnchorPane" prefHeight="500.0" prefWidth="1314.0" style="-fx-background-color: white;"
                VBox.vgrow="ALWAYS">
        <Canvas fx:id="canvas" height="549.0" width="1314.0" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </AnchorPane>
</VBox>
